{
  "expo": {
    "name": "logirate",
    "slug": "logirate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "logirate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash1.png",
      "resizeMode":"cover",
      "backgroundColor":"#0F141A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash1.png",
        "backgroundColor": "#0F141A"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash1.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#0F141A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
